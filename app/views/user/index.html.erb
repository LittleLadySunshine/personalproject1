<div class="col-md-3">
  <h1>Users</h1>
</div>


<div class= "container_buttons pull-right">
    <%= link_to 'Create User', new_user_path, class: "btn btn-info" %>
</div>

<table class="table">


  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Parent Last Name</th>
      <th>Instrument</th>
      <th>Email</th>
    </tr>
  </thead>

  <tbody>
    <% @user.each do |user| %>
      <tr>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.parent_last_name %></td>
        <td><%= user.instrument %></td>
        <td><%= user.email %></td>

        <td><%= link_to 'Edit', edit_user_path(user) %></td>

      </tr>
    <% end %>
  </tbody>
</table>
